<?xml version='1.0' encoding='utf-8'?>
<interface version="1.0">
  <object class="pygubu.builder.widgets.dialog" id="mainwindow">
    <property name="borderwidth">4</property>
    <property name="geometry">500x480</property>
    <property name="height">100</property>
    <property name="minsize">320|240</property>
    <property name="modal">true</property>
    <property name="resizable">both</property>
    <property name="title" translatable="yes">Script Generator</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Frame" id="main">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="newtop">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Frame" id="Frame_1">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Label" id="lbl_tpldesc">
                    <property name="text" translatable="yes">Template description here.</property>
                    <property name="textvariable">string:template_desc_var</property>
                    <property name="wraplength">400</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="pady">5 10</property>
                      <property name="side">bottom</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Label" id="lbl_template">
                    <property name="text" translatable="yes">Template:</property>
                    <layout manager="pack">
                      <property name="padx">0 5</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Radiobutton" id="Radiobutton_1">
                    <property name="command">on_template_changed</property>
                    <property name="style">Toolbutton</property>
                    <property name="text" translatable="yes">Application</property>
                    <property name="value">application</property>
                    <property name="variable">string:template_var</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Separator" id="Separator_1">
                    <property name="orient">vertical</property>
                    <layout manager="pack">
                      <property name="fill">y</property>
                      <property name="padx">5</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Radiobutton" id="Radiobutton_2">
                    <property name="command">on_template_changed</property>
                    <property name="style">Toolbutton</property>
                    <property name="text" translatable="yes">Code Script</property>
                    <property name="value">codescript</property>
                    <property name="variable">string:template_var</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Separator" id="Separator_2">
                    <property name="orient">vertical</property>
                    <layout manager="pack">
                      <property name="fill">y</property>
                      <property name="padx">5</property>
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Radiobutton" id="Radiobutton_3">
                    <property name="command">on_template_changed</property>
                    <property name="style">Toolbutton</property>
                    <property name="text" translatable="yes">Custom Widget</property>
                    <property name="value">widget</property>
                    <property name="variable">string:template_var</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Labelframe" id="lfoptions">
                <property name="height">200</property>
                <property name="padding">4</property>
                <property name="text" translatable="yes">Options:</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="tk.Label" id="lbl_wlist">
                    <property name="text" translatable="yes">Main widget:</property>
                    <layout manager="grid">
                      <property name="sticky">w</property>
                      <property type="col" id="0" name="pad">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="pygubu.builder.widgets.combobox" id="widgetlist">
                    <property name="keyvariable">string:widgetlist_keyvar</property>
                    <property name="textvariable">string:widgetlistvar</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">0</property>
                      <property name="sticky">ew</property>
                      <property type="col" id="1" name="pad">0</property>
                      <property type="col" id="1" name="weight">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="lbl_class">
                    <property name="text" translatable="yes">Class Name:</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="row">1</property>
                      <property name="sticky">w</property>
                      <property type="col" id="0" name="pad">5</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Entry" id="classname">
                    <property name="textvariable">string:classnamevar</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="row">1</property>
                      <property name="sticky">ew</property>
                      <property type="col" id="1" name="pad">0</property>
                      <property type="col" id="1" name="weight">2</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Label" id="spacerlbl">
                    <layout manager="grid">
                      <property name="column">2</property>
                      <property name="row">0</property>
                      <property type="col" id="2" name="weight">1</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="Frame_3">
                <property name="height">200</property>
                <property name="padding">0 4</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="tk.Button" id="btn_generate">
                    <property name="command">on_generate_action</property>
                    <property name="text" translatable="yes">Generate</property>
                    <layout manager="pack">
                      <property name="side">left</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="middle">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="expand">true</property>
              <property name="fill">both</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Frame" id="Frame_5">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="expand">true</property>
                  <property name="fill">both</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="ttk.Label" id="Label_1">
                    <property name="text" translatable="yes">Preview</property>
                    <layout manager="pack">
                      <property name="fill">x</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="pygubu.builder.widgets.scrollbarhelper" id="scrollbarhelper_1">
                    <property name="scrolltype">both</property>
                    <property name="usemousewheel">true</property>
                    <layout manager="pack">
                      <property name="expand">true</property>
                      <property name="fill">both</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                    <child>
                      <object class="tk.Text" id="txt_code">
                        <property name="font">TkFixedFont</property>
                        <property name="height">10</property>
                        <property name="width">50</property>
                        <layout manager="grid">
                          <property name="column">0</property>
                          <property name="propagate">True</property>
                          <property name="row">0</property>
                          <property name="sticky">nsew</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="tk.Frame" id="btnbar2">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="fill">x</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="tk.Button" id="btn_toclipboard">
                    <property name="command">on_copy_action</property>
                    <property name="text" translatable="yes">Copy to Clipboard</property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="tk.Button" id="btn_save">
                    <property name="command">on_save_action</property>
                    <property name="text" translatable="yes">Save</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="bottom">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Button" id="btnclose">
                <property name="command">on_dialog_close</property>
                <property name="text" translatable="yes">Close</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">right</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
