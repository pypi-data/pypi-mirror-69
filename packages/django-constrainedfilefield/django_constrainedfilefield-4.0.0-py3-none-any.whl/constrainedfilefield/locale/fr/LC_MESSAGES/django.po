#: fields.py:42
msgid "A file field with constraints on size and/or type"
msgstr "Un champ de fichier avec des contraintes de taille et/ou type"

#: fields.py:60
#, python-format
msgid "File size exceeds limit: %(current_size)s. Limit is %(max_size)s."
msgstr "Le fichier dépasse la taille autorisée: %(current_size)s. La limite est %(max_size)s."

#: fields.py:79
#, python-format
msgid "Unsupported file type: %(type)s. Allowed types are %(allowed)s."
msgstr "Type de fichier non supporté: %(type)s. Les types autorisés sont %(allowed)s."
