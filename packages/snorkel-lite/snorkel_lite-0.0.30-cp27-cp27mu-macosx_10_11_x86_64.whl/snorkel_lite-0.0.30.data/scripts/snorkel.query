#!python
from __future__ import print_function
import os
import sys

import snorkel
snorkel.shared_mode()

SYBIL_BIN = os.path.join(os.path.dirname(__file__), "snorkel.sybil")

def main():
    args = [SYBIL_BIN, "query"] + sys.argv[1:]
    print(snorkel.run_command(args))

if __name__ == "__main__":
    main()

