<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="ftw.iframeblock">

    <browser:resourceDirectory
        name="ftw.iframeblock"
        directory="resources" />

    <browser:page
        for="ftw.iframeblock.contents.interfaces.IIFrameBlock"
        name="block_view"
        permission="zope2.View"
        class=".iframeblock.IFrameBlockView"
        />

</configure>
