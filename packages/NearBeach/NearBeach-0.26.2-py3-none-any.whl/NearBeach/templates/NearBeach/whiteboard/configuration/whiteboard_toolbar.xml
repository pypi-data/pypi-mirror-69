{% load static %}
<mxDefaultToolbar>
    <add as="save" action="save" icon="{% static 'NearBeach/whiteboard/images/save.gif' %}"/>
	<add as="show" action="show" icon="{% static 'NearBeach/whiteboard/images/preview.gif' %}"/>
	<add as="print" action="print" icon="{% static 'NearBeach/whiteboard/images/print.gif' %}"/>
	<add as="exportImage" action="exportImage" icon="{% static 'NearBeach/whiteboard/images/image.gif' %}"/>
	<br/><hr/>
	<add as="select" mode="select" icon="{% static 'NearBeach/whiteboard/images/select.gif' %}"/>
	<add as="pan" mode="pan" icon="{% static 'NearBeach/whiteboard/images/pan.gif' %}"/>
	<add as="connect" mode="connect" icon="{% static 'NearBeach/whiteboard/images/connect.gif' %}"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = null;
			}
		}
	]]></add>
	<add as="connect" mode="connect" icon="{% static 'NearBeach/whiteboard/images/straight.gif' %}"><![CDATA[
		function (editor)
		{
			if (editor.defaultEdge != null)
			{
				editor.defaultEdge.style = 'straightEdge';
			}
		}
	]]></add>
	<br/><hr/>
	<add as="undo" action="undo" icon="{% static 'NearBeach/whiteboard/images/undo.gif' %}"/>
	<add as="redo" action="redo" icon="{% static 'NearBeach/whiteboard/images/redo.gif' %}"/>
	<br/><hr/>
	<add as="cut" action="cut" icon="{% static 'NearBeach/whiteboard/images/cut.gif' %}"/>
	<add as="copy" action="copy" icon="{% static 'NearBeach/whiteboard/images/copy.gif' %}"/>
	<add as="paste" action="paste" icon="{% static 'NearBeach/whiteboard/images/paste.gif' %}"/>
	<add as="delete" action="delete" icon="{% static 'NearBeach/whiteboard/images/delete.gif' %}"/>
	<br/><hr/>
	<add as="group" action="group" icon="{% static 'NearBeach/whiteboard/images/group.gif' %}"/>
	<add as="ungroup" action="ungroup" icon="{% static 'NearBeach/whiteboard/images/ungroup.gif' %}"/>
	<br/><hr/>
	<add as="Swimlane" template="swimlane" icon="{% static 'NearBeach/whiteboard/images/swimlane.gif' %}"/>
	<add as="Task" template="task" icon="{% static 'NearBeach/whiteboard/images/rectangle.gif' %}"/>
	<add as="Subprocess" template="subprocess" icon="{% static 'NearBeach/whiteboard/images/rounded.gif' %}"/>
	<add as="Ellipse" template="shape" style="ellipse" icon="{% static 'NearBeach/whiteboard/images/ellipse.gif' %}"/>
	<add as="Rhombus" template="shape" style="rhombus" icon="{% static 'NearBeach/whiteboard/images/rhombus.gif' %}"/>
	<add as="Actor" template="shape" style="actor" icon="{% static 'NearBeach/whiteboard/images/actor.gif' %}"/>
	<br/><hr/>
	<add as="Event" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/small_event.gif' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_event.gif' %}"/>
	<add as="Event (Intermediate)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/small_event_intermediate.gif' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_event_intermediate.gif' %}"/>
	<add as="Event (End)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/event_end.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_event_end.gif' %}"/>
	<add as="Timer" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/timer.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_timer.gif' %}"/>
	<add as="Message" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/message.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_message.gif' %}"/>
	<add as="Message (Intermediate)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/message_intermediate.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_message_intermediate.gif' %}"/>
	<add as="Message (End)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/message_end.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_message_end.gif' %}"/>
	<add as="Terminate" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/terminate.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_terminate.gif' %}"/>
	<add as="Link" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/link.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_link.gif' %}"/>
	<add as="Rule" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/rule.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_rule.gif' %}"/>
	<add as="Multiple" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/multiple.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_multiple.gif' %}"/>
	<add as="Error" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/error.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_error.gif' %}"/>
	<add as="Cancel (End)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/cancel_end.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_cancel_end.gif' %}"/>
	<add as="Cancel (Intermediate)" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/cancel_intermediate.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_cancel_intermediate.gif' %}"/>
	<add as="Fork" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/fork.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_fork.gif' %}"/>
	<add as="Merge" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/merge.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_merge.gif' %}"/>
	<add as="Inclusive" template="symbol" style="symbol;image={% static 'NearBeach/whiteboard/images/symbols/inclusive.png' %}" icon="{% static 'NearBeach/whiteboard/images/symbols/small_inclusive.gif' %}"/>
	<br/><hr/>
	<add as="fit" action="fit" icon="{% static 'NearBeach/whiteboard/images/zoom.gif' %}"/>
	<add as="zoomIn" action="zoomIn" icon="{% static 'NearBeach/whiteboard/images/zoomin.gif' %}"/>
	<add as="zoomOut" action="zoomOut" icon="{% static 'NearBeach/whiteboard/images/zoomout.gif' %}"/>
	<add as="actualSize" action="actualSize" icon="{% static 'NearBeach/whiteboard/images/zoomactual.gif' %}"/>
	<add as="zoom" action="zoom" icon="{% static 'NearBeach/whiteboard/images/zoom.gif' %}"/>
	<br/><hr/>
	<add as="outline" action="toggleOutline" icon="{% static 'NearBeach/whiteboard/images/outline.gif' %}"/>
	<add as="tasks" action="toggleTasks" icon="{% static 'NearBeach/whiteboard/images/tasks.gif' %}"/>
	<add as="help" action="toggleHelp" icon="{% static 'NearBeach/whiteboard/images/help.gif' %}"/>
	<add as="console" action="toggleConsole" icon="{% static 'NearBeach/whiteboard/images/console.gif' %}"/>
</mxDefaultToolbar>
