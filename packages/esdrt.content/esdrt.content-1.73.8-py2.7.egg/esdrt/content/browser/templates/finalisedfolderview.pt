        <div class="tabs">
          <div>
            <a class="eea-icon overview" tal:attributes="href string:${here/absolute_url}/view">Overview list</a>
          </div>
          <div>
            <a class="eea-icon inbox" tal:attributes="href string:${here/absolute_url}/inboxview">My view</a>
          </div>
