              <tr>
                <th style="width:17%">
                  Observation
                </th>
                <th style="width:45%">
                    In short
                </th>
                <th>
                  CRF code
                </th>
                <th style="width:80px">
                  Modification date
                </th>
                <th>
                  Step
                </th>
              </tr>
            </thead>

            <tbody>
              <tal:block tal:repeat="item batch1">
